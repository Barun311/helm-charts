groups:
- name: CronusClientAlerts
  rules:
  - alert: InterceptionBodyOnError
    expr: limes_swift_objects_per_container{container_name=~"cronus_inspections.*"} > 0
    for: 5m
    labels:
      service: email
      severity: warning
      tier: os
      support_group: email
    annotations:
      description: cronus intercept email body due to error, special BYD case, please review in ccadmin/master/object-storage
      summary: cronus intercept email body due to error, special BYD case, please review in ccadmin/master/object-storage