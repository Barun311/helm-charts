groups:
- name: logs
  rules:      
  - record: fluentd_input_namespace_records_total
    expr: sum by (input_namespace,hostname,nodename,cluster_type) (fluentd_input_status_num_records_total)
  - record: fluentd_output_namespace_records_total
    expr: sum by (input_namespace,hostname,nodename,cluster_type) (fluentd_output_status_num_records_total)
