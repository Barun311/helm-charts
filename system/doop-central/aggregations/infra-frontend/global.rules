# vim: set ft=yaml:
groups:
- name: doop-central
  rules:
    - record: global:doop_grouped_violations
      expr: 'max by (template_kind, constraint_name) (doop_grouped_violations)'
    - record: global:doop_raw_violations
      expr: 'max by (template_kind, constraint_name, cluster) (doop_raw_violations)'
